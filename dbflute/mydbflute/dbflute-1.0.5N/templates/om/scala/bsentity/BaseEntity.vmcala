    override protected def doBuildColumnString(dm: String): String = {
        val sb: StringBuilder = new StringBuilder();
#foreach ($col in $table.columns)
#if ($col.isJavaNativeUtilDate())
        sb.append(dm).append(xfUD(get${col.javaName}()));
#elseif ($col.isJavaNativeByteArray())
        sb.append(dm).append(xfBA(get${col.javaName}()));
#else
        sb.append(dm).append(get${col.javaName}());
#end
#end
        if (sb.length() > dm.length()) {
            sb.delete(0, dm.length());
        }
        sb.insert(0, "{").append("}");
        return sb.toString();
    }

