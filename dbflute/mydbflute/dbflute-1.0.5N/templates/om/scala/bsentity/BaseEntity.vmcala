    def acceptImmutable(immu: ${myImmutableExtendedObjectClassName}): ${myExtendedObjectClassName} = {
#foreach ($col in $table.columns)
        set${col.javaName}${col.classificationMethodSuffix}(${col.convertToMutablePropertyValue("immu.${col.uncapitalisedJavaName}")});
#end
#foreach ($fk in $table.foreignKeys)
        set${fk.foreignJavaBeansRulePropertyNameInitCap}(immu.${fk.foreignPropertyName}.map(new ${fk.foreignTableExtendedEntityClassName}().acceptImmutable(_)))
#end
#foreach ($referrer in $table.referrerAsOneList)
        set${referrer.referrerJavaBeansRulePropertyNameInitCapAsOne}(immu.${referrer.referrerPropertyNameAsOne}.map(new ${referrer.referrerTableExtendedEntityClassName}().acceptImmutable(_)))
#end
#foreach ($referrer in $table.referrerAsManyList)
        set${referrer.referrerJavaBeansRulePropertyNameInitCap}(immu.${referrer.referrerPropertyName}.map(new ${referrer.referrerTableExtendedEntityClassName}().acceptImmutable(_)).asJava)
#end
        __uniqueDrivenProperties.clear();
        immu.getMyUniqueDrivenProperties().foreach(__uniqueDrivenProperties.addPropertyName(_))
        __modifiedProperties.clear();
        immu.getMyModifiedProperties().foreach(__modifiedProperties.addPropertyName(_))
        return this.asInstanceOf[${myExtendedObjectClassName}];
    }

    /**
     * {@inheritDoc}
     */
    def toImmutable(): ${myImmutableExtendedObjectClassName} = {
        return new ${myImmutableExtendedObjectClassName}(this.asInstanceOf[${myExtendedObjectClassName}]);
    }

